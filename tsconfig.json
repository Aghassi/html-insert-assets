{
    "extends": "@tsconfig/node10/tsconfig.json",

    "include": ["src/**/*.ts"],
    "compilerOptions": {
        "outDir": "dist",
        "baseUrl": "src",
        "declaration": false,
        "noImplicitAny": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
    }
}